{
  "name": "graphql-rest-proxy",
  "version": "0.4.6",
  "description": "Turn your REST API into GraphQL - A Proxy Server that pipes request from GraphQL to REST with GraphQL DSL, performant nested children, mutations, input types, and more.",
  "main": "dist/index.js",
  "repository": "git@github.com:acro5piano/graphql-rest-proxy",
  "author": "gosho-kazuya <ketsume0211@gmail.com>",
  "license": "MIT",
  "keywords": [
    "cli-app",
    "expressjs",
    "graphql",
    "graphql-server",
    "javascript",
    "proxy",
    "rest",
    "rest-api",
    "typescript"
  ],
  "scripts": {
    "start": "npm run ts-run ./src/index.ts",
    "ts-run": "ts-node -T -r tsconfig-paths/register --project tsconfig.dev.json",
    "dev": "nodemon",
    "mock": "npm run ts-run src/__tests__/run-mock-server.ts",
    "prepublish": "npm-run-all clean build",
    "publish": "node ./node_modules/.bin/pack publish",
    "clean": "rm -rf dist/*",
    "build": "pack build",
    "try": "npm-run-all --parallel mock try:proxy",
    "try:proxy": "npm run ts-run ./src/index.ts -c examples/proxy.config.js examples/schema.graphql",
    "test": "cross-env NODE_ENV=testing jest",
    "test:coverage": "cross-env NODE_ENV=testing jest --coverage"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^4.1.0",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "uuid": "^8.3.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@pika/pack": "^0.3.5",
    "@pika/plugin-build-node": "^0.3.14",
    "@pika/plugin-simple-bin": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.3.14",
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.16.1",
    "@types/get-port": "^4.0.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.4",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "get-port": "^5.1.1",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.3.4000"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "graphql-rest-proxy",
          "minNodeVersion": 8
        }
      ]
    ]
  }
}
